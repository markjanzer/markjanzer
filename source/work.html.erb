    <div class="rellax" data-rellax-speed="2">
      <div class="row">
        <div class="col-xs-24 col-sm-21 col-md-16 col-lg-15">
          <h1 class="work-header__title">We brand, build, design and market, and can help you with your next project at just about any stage.</h1>
          <% link_to "/work/", class: "work-header__link" do %>projects <span class="work-header__link-arrow">&#8595;</span><% end %>
        </div>
        <div class="col-xs-24 col-sm-3 col-md-8 col-lg-9">
          <h4 class="work-header__tagline">Our work</h4>
        </div>
      </div>
    </div>
  </div>
