---
itemtype: WebPage
changefreq: "daily"
priority: 1.0
---

<%# Meta Tags %>
<% content_for :meta do %>
  <%= partial("partials/head/meta_tags", locals: {
    local_type:        "website",
    local_card:        "summary_large_image",
    local_image:       "monograph-hero.jpg",
    local_title:       "Process — Dixon & Moe",
    local_description: "Branding, development and design."
  }) %>
<% end %>

<%# Process Header %>
<header class="process-header parallax__group">
  <div class="parallax__layer parallax__layer--0">
    <div class="process-header__text">
      <div class="row">
        <div class="col-xs-24 col-sm-21 col-md-16 col-lg-12 col-lg-offset-2">
          <h1 class="page-header__title">Process.</h1>
        </div>
      </div>
    </div>
  </div>
  <div class="parallax__layer parallax__layer--2">
    <div class="process-graphic__grid row">
      <div class="col-xs-24">
        <span class="process-graphic__grid-col grid-col-1-a"></span>
        <span class="process-graphic__grid-col grid-col-1-b"></span>
        <span class="process-graphic__grid-col grid-col-2-a"></span>
        <span class="process-graphic__grid-col grid-col-2-b"></span>
        <span class="process-graphic__grid-col grid-col-3-a"></span>
        <span class="process-graphic__grid-col grid-col-3-b"></span>
        <span class="process-graphic__grid-col grid-col-4-a"></span>
        <span class="process-graphic__grid-col grid-col-4-b"></span>
        <span class="process-graphic__grid-col grid-col-5-a"></span>
        <span class="process-graphic__grid-col grid-col-5-b"></span>
        <span class="process-graphic__grid-col grid-col-6-a"></span>
        <span class="process-graphic__grid-col grid-col-6-b"></span>
        <span class="process-graphic__grid-col grid-col-7-a"></span>
        <span class="process-graphic__grid-col grid-col-7-b"></span>
        <span class="process-graphic__grid-col grid-col-8-a"></span>
        <span class="process-graphic__grid-col grid-col-8-b"></span>
        <span class="process-graphic__grid-col grid-col-9-a"></span>
        <span class="process-graphic__grid-col grid-col-9-b"></span>
        <span class="process-graphic__grid-col grid-col-10-a"></span>
        <span class="process-graphic__grid-col grid-col-10-b"></span>
        <span class="process-graphic__grid-col grid-col-11-a"></span>
        <span class="process-graphic__grid-col grid-col-11-b"></span>
        <span class="process-graphic__grid-col grid-col-12-a"></span>
        <span class="process-graphic__grid-col grid-col-12-b"></span>
        <span class="process-graphic__grid-col grid-col-13-a"></span>
      </div>
    </div>
    <div class="process-graphic__phases row">
      <div class="col-xs-4 col-xs-offset-2">
        <h4 class="process-graphic__phase-title" style="margin-left:16px;">discovery</h4>
        <h5 class="process-graphic__phase-percent">00%</h5>
      </div>
      <div class="col-xs-4">
        <h4 class="process-graphic__phase-title" style="margin-left:8px;margin-top:4px;">design</h4>
        <h5 class="process-graphic__phase-percent" style="margin-left:-8px;margin-top:-4px;">15%</h5>
      </div>
      <div class="col-xs-4 col-xs-offset-2">
        <h4 class="process-graphic__phase-title" style="margin-left:-8px;margin-top:12px;">development</h4>
        <h5 class="process-graphic__phase-percent" style="margin-left:-16px;margin-top:-12px;">55%</h5>
      </div>
      <div class="col-xs-4">
        <h4 class="process-graphic__phase-title" style="margin-left:-16px;margin-top:16px;">content</h4>
        <h5 class="process-graphic__phase-percent" style="margin-left:-24px;margin-top:-16px;">85%</h5>
      </div>
      <div class="col-xs-4">
        <h4 class="process-graphic__phase-title" style="margin-left:-32px;margin-top:24px;">testing</h4>
        <h5 class="process-graphic__phase-percent" style="margin-left:-32px;margin-top:-24px;">95%</h5>
      </div>
    </div>
  </div>
  <div class="parallax__layer parallax__layer--1">
    <div class="process-graphic__shapes">
      <ul class="js-parallax-scene">
        <li class="process-graphic__shape layer" data-depth="0.20"><%= image_tag("process/shape-content-lg.svg", class: "process-graphic__shape-content") %></li>
        <li class="process-graphic__shape layer" data-depth="0.36"><%= image_tag("process/shape-discovery-lg.svg", class: "process-graphic__shape-discovery") %></li>
        <li class="process-graphic__shape layer" data-depth="0.52"><%= image_tag("process/shape-development-lg.svg", class: "process-graphic__shape-development") %></li>
        <li class="process-graphic__shape layer" data-depth="0.68"><%= image_tag("process/shape-design-lg.svg", class: "process-graphic__shape-design") %></li>
        <li class="process-graphic__shape layer" data-depth="0.84"><%= image_tag("process/shape-testing-lg.svg", class: "process-graphic__shape-testing") %></li>
        <li class="process-graphic__shape layer" data-depth="1.00"><%= image_tag("process/shape-start-lg.svg", class: "process-graphic__shape-start") %></li>
      </ul>
    </div>
  </div>
</header>

<%# Discovery %>
<section class="process-phase parallax__group" style="margin-top: 150vh;">
  <div class="parallax__layer parallax__layer--0">
    <div class="process-phase__text">
      <div class="row">
        <div class="col-lg-8 col-lg-offset-2">
          <h2 class="process-phase__title">Discovery</h2>
          <h4 class="process-phase__subtitle">phase one</h4>
        </div>
        <div class="col-lg-9">
          <p class="process-phase__description">Websites today are both destination and conduit; they must provide a delightful, memorable user experience, and in parallel operate as the primary distribution network for an organization’s online brand and content.</p>
        </div>
      </div>
    </div>
  </div>
  <div class="parallax__layer parallax__layer--1">
    <%= image_tag("process/shape-discovery-sm.svg", class: "process-phase__shape-discovery") %>
  </div>
</section>
